@model CategoryVM
<style>
    #printerDiv iframe {
        position: absolute;
        top: -1000px;
    }
</style>
<div class="jumbotron">
    <button onclick="funExportToPDF()">Export to PDF</button>
</div>
<input type="hidden" asp-for="Id" />
<div id="divPDF">
    <div id="printerDiv"><iframe id="frmPDF"></iframe></div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

<script type="text/javascript">


    function funExportToPDF() {
        debugger
        var txtId = document.getElementById("Id");
        var id = txtId.value;
        var baseUrl = "";
        $.ajax({
            @*url: '@Url.Action("PrintTraineeDataUpgradeFile", "Report",new { id= "rrr"})',*@
            url: baseUrl + "/Report/PrintTraineeDataUpgradeFile?id=" + id,

            type: 'GET',
            contentType: 'text/plain',
            success: function (result) {
                debugger
                $('#frmPDF').attr('src', '@Url.Content("~/TempFiles/")' + result);

                setTimeout(function () {
                    frame = document.getElementById("frmPDF");
                    framedoc = frame.contentWindow;
                    framedoc.focus();
                    framedoc.print();
                }, 1000);
            },
            error: function (xhr, status, err) {
                alert(err);
            }
        });

        return false;
    }

