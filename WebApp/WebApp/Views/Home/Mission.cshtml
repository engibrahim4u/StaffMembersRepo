@{
    ViewData["Title"] = localizer["Mission"];
}

@using System.Globalization

<div class="container">
    <div class="main-image-inside">
        <img src="~/EventImages/journal_logo_no_bg.png" style="max-width:100%" />
    </div>
    <h1 class="font-kawla main-widget">
        <span class="glyphicon glyphicon-folder-open font-40B "></span> @localizer["Mission"]
    </h1>

    <div class="row">
        <ul class="list-group">
            <li class="list-group-item" id="bodyData">
                @if (CultureInfo.CurrentCulture.TextInfo.IsRightToLeft)
                {
                    <div>
                        <ul class="font-18">
                            <li>
                                <h4>
                                    مركز الدراسات الاستراتيجية واعداد القادة بجامعة المنوفية وحدة ذات طابع خاص ، وهو بيت خبرة يقدم منظومة متكاملة  من الخدمات الاستشارية  والبحثية والتدريبية بهدف تحقيق رسالة الجامعة وأهدافها الاستراتيجية من خلال تفعيل العلاقة بين الجامعة وقطاعات المجتمع المحلي والقومي بما يخدم أهداف التنمية الشاملة ، وتقديم الخدمات للمجتمع المحلي والقومي والدولي بمستوى عالي من المهنية والاحتراف وذلك من خلال الاستعانة بما لديه من خبرات أكاديمية ومهنية وادارية على مستوى عال من الكفاءة
                                    واعداد جيل من القادة
                                </h4>
                            </li>
                           
                        </ul>

                    </div>
                }
                else
                {
            <div>
                <ul class="font-18">
                    <li>
                        <h4>
                            مركز الدراسات الاستراتيجية واعداد القادة بجامعة المنوفية وحدة ذات طابع خاص ، وهو بيت خبرة يقدم منظومة متكاملة  من الخدمات الاستشارية  والبحثية والتدريبية بهدف تحقيق رسالة الجامعة وأهدافها الاستراتيجية من خلال تفعيل العلاقة بين الجامعة وقطاعات المجتمع المحلي والقومي بما يخدم أهداف التنمية الشاملة ، وتقديم الخدمات للمجتمع المحلي والقومي والدولي بمستوى عالي من المهنية والاحتراف وذلك من خلال الاستعانة بما لديه من خبرات أكاديمية ومهنية وادارية على مستوى عال من الكفاءة
                            واعداد جيل من القادة
                        </h4>
                    </li>
                </ul>

            </div>

                }
            </li>
        </ul>
    </div>
</div>
